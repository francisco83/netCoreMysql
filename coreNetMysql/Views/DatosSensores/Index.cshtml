@{
}
@model IEnumerable<coreNetMysql.Models.DatosSensores>

<table>
    <tr>
        <th>Fecha</th>
        <th>Temperatura</th>
        <th>Humedad</th>
        <th>Luminosidad</th>
        <th>Volts Panel</th>
        <th>Volts Bateria</th>
    </tr>

    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Fecha</td>
            <td>@item.Temperatura</td>
            <td>@item.Humedad</td>
            <td>@item.Luminosidad</td>
            <td>@item.Voltspanel</td>
            <td>@item.Voltsbateria</td>
        </tr>

    }

</table>